<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Shutters">
        <attr name="window_width" format="dimension" />
        <attr name="window_border_thickness" format="dimension" />
        <attr name="shutter_part_amount" format="integer" />
        <attr name="shutter_gap_percent_of_part" format="integer" />
        <attr name="android:src" />
    </declare-styleable>

    <declare-styleable name="Picker">
        <attr name="color_wheel_radius" format="dimension" />
        <attr name="color_wheel_thickness" format="dimension" />
        <attr name="color_wheel_selected_thickness" format="dimension" />
        <attr name="color_center_radius" format="dimension" />
        <attr name="color_center_halo_radius" format="dimension"/>
        <attr name="color_pointer_radius" format="dimension" />
        <attr name="color_pointer_halo_radius" format="dimension" />
        <attr name="show_wheel_selected" format="boolean" />
        <attr name="color_text" format="color" />
        <attr name="color_disabled" format="color" />
        <attr name="color_selected" format="color" />
    </declare-styleable>

</resources>